{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "css.customData": [
    ".vscode/css-data.json"
  ],
  "editor.codeActionsOnSave": [
    "source.sortImports"
  ],
  "eslint.format.enable": false,
  "tailwindCSS.experimental.configFile": "src/styles/tailwind.css"
}